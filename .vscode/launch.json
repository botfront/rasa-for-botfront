    "version": "0.2.0",
    "configurations": [
        {
            "name": "Rasa server",
            "type": "python",
            "request": "launch",
            "module": "rasa",
            "cwd": "/User/{yourUserName}/{bfProject}/models",
            "args": ["run", "-m", "/User/{yourUserName}/{bfProject}/models", "--debug", "--enable-api"],
            "env": {
                "BF_PROJECT_ID": "bf",
                "BF_URL": "http://localhost:3000/graphql",
            }
        }
    ]
}
